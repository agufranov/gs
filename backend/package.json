{
  "name": "gs-backend",
  "version": "1.0.0",
  "description": "Backend API for GS project using TypeScript and Fastify",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "prisma:generate": "cd ../packages/shared && prisma generate",
    "prisma:migrate": "cd ../packages/shared && prisma migrate dev",
    "prisma:studio": "cd ../packages/shared && prisma studio",
    "prisma:deploy": "cd ../packages/shared && prisma migrate deploy"
  },
  "keywords": [
    "fastify",
    "typescript",
    "api",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^6.0.0",
    "@gs/shared": "file:../packages/shared",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "fastify": "^5.6.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.10.0",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.19.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
